{
  "name": "blankbrackets-postgres-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for AI-assisted PostgreSQL database analysis and optimization. Provides read-only access for comprehensive performance analysis, index optimization, and schema recommendations.",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "postgres-mcp": "build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "postgres",
    "postgresql",
    "database",
    "analysis",
    "optimization",
    "performance",
    "ai",
    "llm",
    "claude",
    "cursor",
    "database-optimization",
    "index-optimization",
    "query-analysis"
  ],
  "author": "Postgres MCP Server Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blankbrackets/postgres-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/blankbrackets/postgres-mcp/issues"
  },
  "homepage": "https://github.com/blankbrackets/postgres-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "pg": "^8.13.1",
    "dotenv": "^16.4.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "types": "./build/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples"
  }
}
